# client
<%- if allow_dotexit != 0 then -%>
AllowDotExit <%= allow_dotexit %>
<%-   end -%>
<%- if allow_single_hop_circuits != 0 then -%>
AllowSingleHopCircuits <%= allow_single_hop_circuits %>
<%-   end -%>
<%- if enforce_distinct_subnets != 1 then -%>
EnforceDistinctSubnets <%= enforce_distinct_subnets %>
<%-   end -%>
<%- if exclude_single_hop_relays != 1 then -%>
ExcludeSingleHopRelays <%= exclude_single_hop_relays %>
<%-   end -%>
<%-   if exclude_nodes != '' then -%>
ExcludeNodes <%= exclude_nodes %>
<%-   end -%>
<%-   if exclude_exit_nodes != '' then -%>
ExcludeExitNodes <%= exclude_exit_nodes %>
<%-   end -%>
<%-   if exit_nodes != '' then -%>
ExitNodes <%= exit_nodes %>
<%-   end -%>
<%-   if entry_nodes != '' then -%>
EntryNodes <%= entry_nodes %>
<%-   end -%>
<%- if new_circuit_period != 30 then -%>
NewCircuitPeriod <%= new_circuit_period %>
<%-   end -%>
<%- if max_circuit_dirtiness != 600 then -%>
MaxCircuitDirtiness <%= max_circuit_dirtiness %>
<%-   end -%>
<%- if strict_nodes != 0 then -%>
StrictNodes <%= strict_nodes %>
<%-   end -%>
<%- if use_entry_guards != 1 then -%>
UseEntryGuards <%= use_entry_guards %>
<%-   end -%>
